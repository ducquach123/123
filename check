-- Execute script: tải và chạy 2 remote scripts (có bắt lỗi)
task.wait(2)

local function fetch_and_exec(url)
    -- cố gắng download
    local ok, res = pcall(function()
        return game:HttpGet(url)
    end)
    if not ok then
        warn('[fetch_and_exec] Lỗi khi HttpGet:', url, res)
        return false, res
    end
    if not res or res == '' then
        warn('[fetch_and_exec] Nội dung rỗng từ:', url)
        return false, 'empty'
    end

    -- cố gắng chạy loadstring
    local ok2, err = pcall(function()
        local fn = loadstring(res)
        if not fn then
            error('loadstring trả về nil')
        end
        fn()
    end)
    if not ok2 then
        warn('[fetch_and_exec] Lỗi khi thực thi script từ:', url, err)
        return false, err
    end

    print('[fetch_and_exec] Thực thi thành công:', url)
    return true
end

-- URL bạn đưa
local url1 = 'https://i.imgur.gg/SM0WygS-blocker.lua'
local url2 =
    'https://raw.githubusercontent.com/tienkhanh1/spicy/main/Chilli.lua'

-- Thực thi theo thứ tự
fetch_and_exec(url1)
fetch_and_exec(url2)
